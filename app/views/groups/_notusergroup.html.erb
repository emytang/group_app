<h4 style="display:inline;">List of NOT Users</h4>

<div class="container">
    <div class="row">
        <table class ="table table-striped table-bordered table-hover">

          <thead class ="thead-dark">
            <tr>
              <th>Actions</th>
              <th>First Name</th>
              <th>Last Name</th>
              <th>Email</th>
            </tr>
          </thead>

          <tbody>
            <%#  @notmembers.empty? %>
            <% @notmembers.each do |member| %>
              <tr>
                <td>
                    <%= link_to 'Add', add_to_group_path(@group, member), class: "btn btn-secondary"%>
                </td>
                <td><%= member.first_name %></td>
                <td><%= member.last_name %></td>
                <td><%= member.email %></td>
              </tr>
            <% end %>
        </tbody>

        </table>
    </div>
</div>